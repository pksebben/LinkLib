        {
            "avatar_url": "https://secure.gravatar.com/avatar/282da691709f4715fc3e0ff6bebd278b?d=identicon&version=1",
            "client": "website",
            "content": "<p>I guess a related question is how to think about passing a frame to a view if you're going to use auto layout constraints.</p>",
            "content_type": "text/html",
            "display_recipient": "iOS",
            "flags": [
                "read",
                "historical"
            ],
            "id": 48285181,
            "is_me_message": false,
