        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>Of course, I'm probably harshing on Mozart/Oz too much; maybe as I start to do the next few sections of the course, I'll start to understand the motivation behind their choices.</p>",
            "content_type": "text/html",
            "display_recipient": "ctm course",
            "flags": [
                "read",
                "historical"
            ],
            "id": 18202398,
            "is_me_message": false,
