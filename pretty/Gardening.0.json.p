        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/c01fa66cd5fa025861dc065d5a18974f2806a9b3?x=x&version=5",
            "client": "ZulipElectron",
            "content": "<p>I'm going to post the list I'm looking at that I just sent along to my family (they promised me a plot of land to try cultivating this spring and I am beyond pumped)</p>",
            "content_type": "text/html",
            "display_recipient": "Gardening",
            "flags": [
                "read",
                "historical"
            ],
            "id": 157987253,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 219168,
