                "historical"
            ],
            "id": 122287171,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 21991,
            "sender_email": "brunerson@gmail.com",
            "sender_full_name": "Amelia Rimera Binotti Riely (F2'17)",
