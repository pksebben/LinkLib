        {
            "avatar_url": "https://secure.gravatar.com/avatar/55ddf2ec93de8a2a31a0f99d0e874ff9?d=identicon&version=1",
            "client": "website",
            "content": "<p>Definitely work under a written contract, which it sounds like you're planning to do = )</p>",
            "content_type": "text/html",
            "display_recipient": "jobs",
            "flags": [
                "read",
                "historical"
            ],
            "id": 179756744,
            "is_me_message": false,
