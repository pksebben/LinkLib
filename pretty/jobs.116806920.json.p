        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/febd921255aa4781446342ec6ccfa6e094cddba2?x=x&version=3",
            "client": "ZulipElectron",
            "content": "<p>This seems like a useful resource - paid tech apprenticeship for underrepresented: <a href=\"https://techtonica.org/apprenticeship/\" target=\"_blank\" title=\"https://techtonica.org/apprenticeship/\">https://techtonica.org/apprenticeship/</a></p>",
            "content_type": "text/html",
