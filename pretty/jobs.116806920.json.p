        {
            "avatar_url": "https://secure.gravatar.com/avatar/55ddf2ec93de8a2a31a0f99d0e874ff9?d=identicon&version=1",
            "client": "website",
            "content": "<p>There is a separate bit of the US tax code that allows you to claim foreign taxes paid as a credit towards your US obligations, which is what prevents \"double taxation\".</p>",
            "content_type": "text/html",
            "display_recipient": "jobs",
            "flags": [
                "read",
                "historical"
            ],
            "id": 203262750,
            "is_me_message": false,
            "reactions": [],
