        {
            "avatar_url": "https://secure.gravatar.com/avatar/55ddf2ec93de8a2a31a0f99d0e874ff9?d=identicon&version=1",
            "client": "website",
            "content": "<p>A big decision is whether to work on a \"time and materials\" basis (i.e., hourly/daily/weekly rate) or on a fixed-price contract for specific deliverables. There have been lots of discussions about this on Zulip and elsewhere.</p>",
            "content_type": "text/html",
            "display_recipient": "jobs",
            "flags": [
                "read",
                "historical"
            ],
            "id": 179756803,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 21991,
            "sender_email": "kevin@keminglabs.com",
            "sender_full_name": "Kevin Lynagh (he)",
            "sender_id": 2172,
            "sender_realm_str": "recurse",
            "stream_id": 19962,
            "subject": "Resources on negotiating a contract",
            "submessages": [],
