            "client": "website",
            "content": "<p>Resources: <a href=\"https://github.com/pksebben/AmazingGrace/blob/main/README.md\">https://github.com/pksebben/AmazingGrace/blob/main/README.md</a></p>",
            "content_type": "text/html",
