        {
            "avatar_url": "https://secure.gravatar.com/avatar/044b59a59f2bfb736b3142db2cd025ca?d=identicon&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"w.aptekar@gmail.com\" data-user-id=\"101330\">@Wesley Aptekar-Cassels (F2'17)</span> XEdDSA is a way to use Montgomery keys (like X25519 keys) to generate EdDSA signatures (like Ed25519 signatures).</p>",
            "content_type": "text/html",
            "display_recipient": "crypto",
            "flags": [
                "read",
                "historical"
            ],
            "id": 116390045,
            "is_me_message": false,
            "reactions": [
                {
                    "emoji_code": "1f44d",
                    "emoji_name": "+1",
                    "reaction_type": "unicode_emoji",
                    "user": {
                        "email": "me@wesleyac.com",
                        "full_name": "Wesley Aptekar-Cassels (they) (m8'20)",
                        "id": 101330
                    },
                    "user_id": 101330
                }
            ],
            "recipient_id": 42288,
            "sender_email": "hi@filippo.io",
            "sender_full_name": "Filippo Valsorda (F2'17)",
            "sender_id": 2884,
            "sender_realm_str": "recurse",
            "stream_id": 27446,
            "subject": "\"EdDSA compatible\"",
            "submessages": [],
            "timestamp": 1506526493,
