            "type": "stream"
        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/8f076e5e03c30a36d598445308926598?d=identicon&version=1",
            "client": "website",
            "content": "<p>I especially find it unfortunate that they implemented their own client API instead of going with ActivityPub, which is the upcoming standard for this stuff</p>",
            "content_type": "text/html",
            "display_recipient": "projects",
            "flags": [
                "read",
                "historical"
            ],
            "id": 109190596,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22713,
            "sender_email": "alex@strugee.net",
            "sender_full_name": "AJ Jordan (he/they) (SP2'17)",
            "sender_id": 100461,
            "sender_realm_str": "recurse",
            "stream_id": 20324,
            "subject": "mastodon",
