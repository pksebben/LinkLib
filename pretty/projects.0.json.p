        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/04ca5a14273f759166bbbfc15e3e400910e7a053?x=x&version=1",
            "client": "website",
            "content": "<p>I realize that I probably err pretty far toward the spam-avoidant end of the spectrum, though.</p>",
            "content_type": "text/html",
            "display_recipient": "projects",
            "flags": [
                "read",
                "historical"
            ],
            "id": 61689604,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22713,
            "sender_email": "jdherg@gmail.com",
            "sender_full_name": "John Hergenroeder (he) (SP2'15)",
            "sender_id": 7851,
            "sender_realm_str": "recurse",
            "stream_id": 20324,
            "subject": "RSVPBot",
            "submessages": [],
