        {
            "avatar_url": "https://secure.gravatar.com/avatar/8f076e5e03c30a36d598445308926598?d=identicon&version=1",
            "client": "website",
            "content": "<p>and so I just find it really unfortunate that they put so much effort into implementing protocols (some of) the decentralized social web community would like to see deprecated</p>",
            "content_type": "text/html",
            "display_recipient": "projects",
            "flags": [
                "read",
                "historical"
            ],
            "id": 109190588,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22713,
            "sender_email": "alex@strugee.net",
            "sender_full_name": "AJ Jordan (he/they) (SP2'17)",
