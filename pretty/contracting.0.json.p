            ],
            "id": 118782644,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 37111,
            "sender_email": "julian@cipht.net",
            "sender_full_name": "Julian Squires (he) (F2'17)",
            "sender_id": 101992,
