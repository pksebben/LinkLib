            "id": 128990879,
            "is_me_message": false,
            "last_edit_timestamp": 1530569903,
            "reactions": [],
            "recipient_id": 37111,
            "sender_email": "liu.brandon@gmail.com",
            "sender_full_name": "Brandon Liu (SP2'18)",
            "sender_id": 111910,
            "sender_realm_str": "recurse",
