        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/612c7c2a108d7850c62d1d2293a481aa42855408?x=x&version=2",
            "client": "website",
            "content": "<p>does anyone have experience contracting, or know the law about starting a business? it seems like the conventional wisdom is completely contrary to my lawyer friends knowledge and its totally stressing me out about getting this contract started!</p>",
            "content_type": "text/html",
            "display_recipient": "contracting",
            "flags": [
                "read",
                "historical"
            ],
            "id": 48154567,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 37111,
            "sender_email": "PLRedmond@gmail.com",
            "sender_full_name": "Patrick Redmond (W'14)",
            "sender_id": 5014,
            "sender_realm_str": "recurse",
