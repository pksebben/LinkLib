        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/d16cd71b27dfd2af0f84e16276c00b61314db772?x=x&version=3",
            "client": "ZulipElectron",
            "content": "<p>yeah, honestly, I like google doc, we're  just curious what else is out there</p>",
            "content_type": "text/html",
            "display_recipient": "engineering management",
            "flags": [
                "read",
