        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/541aab26abf25ac195df55d7e6c0f2daf3f00c70?x=x&version=1",
            "client": "website",
            "content": "<p>I just read <a href=\"https://hbr.org/2019/04/make-your-meetings-a-safe-space-for-honest-conversation\">https://hbr.org/2019/04/make-your-meetings-a-safe-space-for-honest-conversation</a> and it struck me as relevant</p>",
            "content_type": "text/html",
            "display_recipient": "engineering management",
            "flags": [
                "read",
                "historical"
            ],
            "id": 203592140,
            "is_me_message": false,
