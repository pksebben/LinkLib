            "last_edit_timestamp": 1533267192,
            "reactions": [],
            "recipient_id": 37230,
            "sender_email": "plapidus@gmail.com",
            "sender_full_name": "Perri Lapidus (she) (SP1'18)",
            "sender_id": 109182,
            "sender_realm_str": "recurse",
