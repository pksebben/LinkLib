        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/db2b0f0c6edcb0af7bca6fd732895c4da72eb35c?x=x&version=2",
            "client": "ZulipElectron",
            "content": "<p>Note: the role will be hands-off in terms of coding so there was no technical interview portion.</p>",
            "content_type": "text/html",
            "display_recipient": "engineering management",
            "flags": [
                "read",
                "historical"
