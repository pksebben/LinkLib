        {
            "avatar_url": "https://secure.gravatar.com/avatar/282da691709f4715fc3e0ff6bebd278b?d=identicon&version=1",
            "client": "website",
            "content": "<p>This is basically just the product of the original category with itself, so maybe that's one reason why it's not the right construction--we want something cooler :)</p>",
            "content_type": "text/html",
            "display_recipient": "category theory",
            "flags": [
                "read",
                "historical"
            ],
            "id": 36663856,
            "is_me_message": false,
            "reactions": [],
