        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/282da691709f4715fc3e0ff6bebd278b?d=identicon&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"dkarapetyan@scriptcrafty.com\">@David Karapetyan (W1'14)</span> <span class=\"user-mention\" data-user-email=\"alexzhu@mit.edu\">@Alex Zhu (SP1'15)</span> <span class=\"user-mention\" data-user-email=\"joelburget@gmail.com\">@Joel Burget (SP1'15)</span> <span class=\"user-mention\" data-user-email=\"jonathantan1991@gmail.com\">@Jonathan Terence Tan (SP1'15)</span> ?</p>",
            "content_type": "text/html",
            "display_recipient": "category theory",
            "flags": [
                "read",
                "historical"
            ],
            "id": 36654647,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 39558,
            "sender_email": "alan.m.odonnell@gmail.com",
            "sender_full_name": "Alan O'Donnell (he) (S1'19)",
            "sender_id": 813,
            "sender_realm_str": "recurse",
            "stream_id": 26833,
            "subject": "The arrow category",
            "submessages": [],
