        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/1588fc084a150438206141abca2a434daa4c8fa5?x=x&version=2",
            "client": "website",
            "content": "<p>The only experiment that bears fruits in mathematics, for me, is finding a counter-example.</p>",
            "content_type": "text/html",
            "display_recipient": "cs-theory",
            "flags": [
                "read",
                "historical"
            ],
            "id": 212747073,
            "is_me_message": false,
            "reactions": [],
