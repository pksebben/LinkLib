        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/1588fc084a150438206141abca2a434daa4c8fa5?x=x&version=2",
            "client": "website",
            "content": "<p>This is my question for now: is computer science invented or discovered?</p>\n<p>Are there good texts on the questions - that might or might not be about the same question, but for mathematics ?</p>",
            "content_type": "text/html",
            "display_recipient": "cs-theory",
            "flags": [
                "read",
                "historical"
            ],
            "id": 212727170,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 563062,
            "sender_email": "finartcialist@gmail.com",
            "sender_full_name": "Edith Viau (she) (F2'20)",
            "sender_id": 341049,
            "sender_realm_str": "recurse",
            "stream_id": 260473,
            "subject": "is cs invented or discovered?",
            "submessages": [],
