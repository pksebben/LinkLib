        {
            "avatar_url": "https://secure.gravatar.com/avatar/3514240cecd66b31ef3b3a4a7ea235cc?d=identicon&version=1",
            "client": "website",
            "content": "<p>i was reading the blog post for undergrads that <span class=\"user-mention\" data-user-email=\"hopan@uchicago.edu\">@Horace Pan (S2'16)</span> mentioned and thought it was straight-up good advice for grad students: </p>",
            "content_type": "text/html",
            "display_recipient": "grad-school",
            "flags": [
                "read",
                "historical"
            ],
            "id": 102895100,
            "is_me_message": false,
            "reactions": [],
