        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/fb9db70f2a2a6ecf06ad7c798a5c9240697c4f20?x=x&version=1",
            "client": "website",
            "content": "<p>cross posted from advice here</p>",
            "content_type": "text/html",
            "display_recipient": "grad-school",
            "flags": [
                "read",
