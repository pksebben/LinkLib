        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/ea5ab7f7d6e19ffdee32487a6e145475c956689b?x=x&version=3",
            "client": "website",
            "content": "<p>Found on Marine Carpaut's homepage. <a href=\"http://www.cs.umd.edu/~marine//teaching/\" target=\"_blank\" title=\"http://www.cs.umd.edu/~marine//teaching/\">http://www.cs.umd.edu/~marine//teaching/</a></p>",
            "content_type": "text/html",
            "display_recipient": "grad-school",
