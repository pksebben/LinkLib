        {
            "avatar_url": "https://secure.gravatar.com/avatar/d922fc1f076ca5480a16c7d56a388a08?d=identicon&version=1",
            "client": "website",
            "content": "<p>x-posting from <a class=\"stream\" data-stream-id=\"31172\" href=\"/#narrow/stream/advice\">#advice</a></p>",
            "content_type": "text/html",
            "display_recipient": "grad-school",
