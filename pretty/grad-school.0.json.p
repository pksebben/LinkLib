            "sender_email": "nathan.schucher@gmail.com",
            "sender_full_name": "Nathan Schucher (F2'16)",
            "sender_id": 100199,
            "sender_realm_str": "recurse",
            "stream_id": 32918,
            "subject": "Masters references",
            "submessages": [],
