        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/c01fa66cd5fa025861dc065d5a18974f2806a9b3?x=x&version=5",
            "client": "ZulipMobile",
            "content": "<p>Investment / payout in this case meaning a sense of well-being and support in the minority community I was a part of compared to the energy I put into the community.</p>",
            "content_type": "text/html",
            "display_recipient": "quiltbag+",
            "flags": [
                "read",
                "historical"
            ],
            "id": 135437165,
            "is_me_message": false,
