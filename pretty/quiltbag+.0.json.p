        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/e5845e045f9bcd4c95b82b01366893ed8b219f00?x=x&version=1",
            "client": "website",
            "content": "<p>Is there any more-or-less standard terminology for this?  If so it might also apply in analogous cases.  For example, one might say that lesbians are female-oriented.</p>",
            "content_type": "text/html",
            "display_recipient": "quiltbag+",
            "flags": [
                "read",
                "historical"
            ],
            "id": 182667183,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 33931,
            "sender_email": "mjd@plover.com",
            "sender_full_name": "Mark Dominus (indifferent)",
            "sender_id": 7912,
            "sender_realm_str": "recurse",
            "stream_id": 25651,
            "subject": "Terminology / identification question",
            "submessages": [],
