        {
            "avatar_url": "https://secure.gravatar.com/avatar/6f8ddedf593eefcabbc13434b183e7d7?d=identicon&version=1",
            "client": "ZulipMobile",
            "content": "<p>I\u2019ve also been there (very Catholic mom, Catholic school from k-12). Sending you all the good vibes. \u2764\ufe0f</p>",
            "content_type": "text/html",
            "display_recipient": "quiltbag+",
