            "sender_full_name": "Christopher Ray Ertel (he) (S1'19)",
            "sender_id": 221817,
            "sender_realm_str": "recurse",
            "stream_id": 25651,
            "subject": "LGBTQ+ Video Games",
            "submessages": [],
