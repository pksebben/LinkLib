        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/e5845e045f9bcd4c95b82b01366893ed8b219f00?x=x&version=1",
            "client": "website",
            "content": "<p>I said maybe \u201cmale-oriented\u201d / \u201cfemale-oriented\u201d, but I also added that I wasn't sure that I hadn't just made that up, and even if I hadn't, I wasn't sure that it was generally understood.</p>",
            "content_type": "text/html",
            "display_recipient": "quiltbag+",
            "flags": [
                "read",
                "historical"
            ],
            "id": 182667094,
            "is_me_message": false,
            "reactions": [],
