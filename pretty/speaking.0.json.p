        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>He may or may not have thought the presentation was great, but regardless, he always shows a huge amount of enthusiasm about the *topic*.</p>",
            "content_type": "text/html",
            "display_recipient": "speaking",
            "flags": [
                "read",
                "historical"
            ],
            "id": 35086271,
            "is_me_message": false,
