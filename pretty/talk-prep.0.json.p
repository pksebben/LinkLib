            "type": "stream"
        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/7d2ab014ebf55e99afffba1a3d92be8155754f0f?x=x&version=1",
            "client": "website",
            "content": "<p>The result is more topics covered, and being able to engage more with the ones that grab you.</p>",
            "content_type": "text/html",
            "display_recipient": "talk-prep",
            "flags": [
                "read",
                "historical"
            ],
