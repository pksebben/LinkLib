            ],
            "id": 54684774,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 42491,
            "sender_email": "gnclmorais@gmail.com",
            "sender_full_name": "Gon\u00e7alo Morais (SP1'15)",
            "sender_id": 7631,
            "sender_realm_str": "recurse",
