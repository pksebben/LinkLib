            "sender_email": "quinnbatten@gmail.com",
            "sender_full_name": "Quinn Batten (he) (F1'18)",
            "sender_id": 125112,
            "sender_realm_str": "recurse",
            "stream_id": 24328,
            "subject": "Recommended reading: Matt Levine\u2019s Money Stuff",
            "submessages": [],
