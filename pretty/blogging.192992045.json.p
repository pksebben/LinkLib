        {
            "avatar_url": "https://secure.gravatar.com/avatar/29581c8174d213b28a7fd1d59b1ce0d8?d=identicon&version=1",
            "client": "website",
            "content": "<p>Thanks!</p>",
            "content_type": "text/html",
            "display_recipient": "blogging",
            "flags": [
                "read",
                "historical"
            ],
            "id": 212472803,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 30248,
            "sender_email": "pstefek2@gmail.com",
            "sender_full_name": "Peter Stefek (he) (F2'20)",
            "sender_id": 329808,
            "sender_realm_str": "recurse",
            "stream_id": 24328,
            "subject": "Fooling Around with Foveated Rendering",
            "submessages": [],
            "timestamp": 1602012058,
