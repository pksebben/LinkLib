        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/04ca5a14273f759166bbbfc15e3e400910e7a053?x=x&version=1",
            "client": "website",
            "content": "<p>This feels conceptually adjacent to <a href=\"https://solid.mit.edu/\">Solid</a>, which includes among other principles:</p>\n<blockquote>\n<p>True data ownership</p>\n<p>Users should have the freedom to choose where their data resides and who is allowed to access it. By decoupling content from the application itself, users are now able to do so.</p>\n</blockquote>",
            "content_type": "text/html",
            "display_recipient": "blogging",
            "flags": [
                "read",
                "historical"
            ],
            "id": 204433596,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 30248,
            "sender_email": "jdherg@gmail.com",
            "sender_full_name": "John Hergenroeder (he) (SP2'15)",
