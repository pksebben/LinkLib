        {
            "avatar_url": "https://secure.gravatar.com/avatar/7aaa8e042f3b1bd30ef04a6259e300c0?d=identicon&version=1",
            "client": "website",
            "content": "<p>Thank you! And that Twitter account's neat, thanks for sharing. I've added it to the post.</p>",
            "content_type": "text/html",
            "display_recipient": "blogging",
            "flags": [
                "read",
                "historical"
            ],
            "id": 213063955,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 30248,
            "sender_email": "buriedunderbooks@hotmail.com",
            "sender_full_name": "Thomas Broadley (he) (m7'20)",
            "sender_id": 341071,
            "sender_realm_str": "recurse",
            "stream_id": 24328,
            "subject": "Misleading with percent changes of percentages",
