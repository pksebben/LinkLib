        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/c6a4682e6a46285de64c97c77b771481?d=identicon&version=1",
            "client": "website",
            "content": "<p>one of the examples is the Unix open()/read()/write()/close() family of APIs, and how it abstracts talking to a ton of different hardware and programs.</p>",
            "content_type": "text/html",
            "display_recipient": "small questions",
            "flags": [
                "read",
                "historical"
            ],
            "id": 214368812,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 38232,
            "sender_email": "ori@eigenstate.org",
