        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/e30b9e7d531d60960da4c58b24cf7e7c5aff03f0?x=x&version=5",
            "client": "ZulipElectron",
            "content": "<p>IANAL but my intuition would be this would be treated as signed \"under coercion\" and therefore not binding.</p>",
            "content_type": "text/html",
            "display_recipient": "small questions",
            "flags": [
                "read",
                "historical"
