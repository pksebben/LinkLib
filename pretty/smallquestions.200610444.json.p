        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/8f3e3f13689ac077fe4ae537e33bde345a57cb22?x=x&version=2",
            "client": "website",
            "content": "<p>(relatedly: how do you bootstrap async execution if you can't call an async function from a regular one?)</p>",
            "content_type": "text/html",
            "display_recipient": "small questions",
            "flags": [
