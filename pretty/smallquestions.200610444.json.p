            "sender_email": "robert@schuessler.org",
            "sender_full_name": "Robert Schuessler (W1'17)",
            "sender_id": 103047,
            "sender_realm_str": "recurse",
            "stream_id": 26440,
            "subject": "C testing setup",
            "submessages": [],
