        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/8d9ed5f596714f4f3946f0896c9247b7686db1f6?x=x&version=4",
            "client": "website",
            "content": "<p>you could have \"hosting coin\" that people spend when they request a specific resource, and people earn/mine it by rehosting that content for others</p>",
            "content_type": "text/html",
            "display_recipient": "the loop",
            "flags": [
