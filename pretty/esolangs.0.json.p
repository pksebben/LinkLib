        {
            "avatar_url": "https://secure.gravatar.com/avatar/27de1e4796502970d91435bdb9f420aa?d=identicon&version=1",
            "client": "website",
            "content": "<p>I've been wondering how a cooperative programming language would look like. One that <em>requires</em> 2 people working together to use it.</p>",
            "content_type": "text/html",
            "display_recipient": "esolangs",
            "flags": [
                "read",
                "historical"
            ],
            "id": 216738026,
            "is_me_message": false,
