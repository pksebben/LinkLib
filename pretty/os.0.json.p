        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/a43129835ecf709dcdca8afc15ac6718bb166fef?x=x&version=2",
            "client": "ZulipElectron",
            "content": "<p>First homework assignment: <a href=\"https://cs162.eecs.berkeley.edu/static/hw/hw0.pdf\" target=\"_blank\" title=\"https://cs162.eecs.berkeley.edu/static/hw/hw0.pdf\">https://cs162.eecs.berkeley.edu/static/hw/hw0.pdf</a></p>",
            "content_type": "text/html",
            "display_recipient": "os",
            "flags": [
                "read",
