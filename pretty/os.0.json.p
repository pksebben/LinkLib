        {
            "avatar_url": "https://secure.gravatar.com/avatar/a6a0ff0a4a9a86ae7bc4524453d439bd?d=identicon&version=1",
            "client": "website",
            "content": "<p>hm I don't see why that's the case though, could you elaborate?</p>",
            "content_type": "text/html",
            "display_recipient": "os",
            "flags": [
                "read",
                "historical"
            ],
            "id": 121097016,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 37095,
            "sender_email": "annie.cherk@gmail.com",
            "sender_full_name": "Annie Cherkaev (she/they) (m4'18)",
            "sender_id": 9502,
            "sender_realm_str": "recurse",
            "stream_id": 26244,
            "subject": "virtual to physical mem mapping",
