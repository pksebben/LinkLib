        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/4c34e314b2c38cb8147d66a6a7dbb793627ea3f6?x=x&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"danluu@gmail.com\">@Dan Luu (W'13)</span> : Oh wow! I know that feeling! I pull out some of my old slides from years ago and there'll be pictures that obviously go with some sort of a joke I had, but I now have *no idea what that joke is*.</p>",
            "content_type": "text/html",
            "display_recipient": "conferences",
            "flags": [
                "read",
                "historical"
            ],
            "id": 62577007,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 41621,
            "sender_email": "paul.j.fenwick@gmail.com",
            "sender_full_name": "Paul Fenwick",
