        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/c55d5b0f6b48fe10eae3bd82fdc9ec479c66f7b7?x=x&version=1",
            "client": "website",
            "content": "<p>Is there any chance of a ticket lottery for a future year to open it up to people who can't just refresh the page at that specific time?</p>",
            "content_type": "text/html",
            "display_recipient": "conferences",
            "flags": [
                "read",
                "historical"
            ],
            "id": 124427818,
            "is_me_message": false,
            "last_edit_timestamp": 1522440844,
            "reactions": [],
            "recipient_id": 41621,
            "sender_email": "msteigerwalt@gmail.com",
            "sender_full_name": "Michelle Steigerwalt (she/her) (W2'17)",
            "sender_id": 7655,
            "sender_realm_str": "recurse",
            "stream_id": 27328,
            "subject": "!!Con 2018 registration",
            "submessages": [],
