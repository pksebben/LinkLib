        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>Ooh, so you have to put a price tag on those things?  That seems really hard to do!</p>",
            "content_type": "text/html",
            "display_recipient": "conferences",
            "flags": [
                "read",
                "historical"
            ],
            "id": 174177605,
            "is_me_message": false,
            "reactions": [],
