        {
            "avatar_url": "https://secure.gravatar.com/avatar/8756446588cfc2014859f5f1fa68c850?d=identicon&version=1",
            "client": "website",
            "content": "<p>(I'm frustrated that I'm having to spend time and energy being outraged about this and advocating for change, instead of being excited about going to the conference.)</p>",
            "content_type": "text/html",
            "display_recipient": "conferences",
            "flags": [
                "read",
                "historical"
            ],
            "id": 172525975,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 41621,
            "sender_email": "jason.a.owen@gmail.com",
            "sender_full_name": "Jason Owen (SP2'19)",
