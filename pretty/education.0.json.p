            "id": 112380612,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 47153,
            "sender_email": "dpb@brannerchinese.com",
            "sender_full_name": "David P Branner (SP2'17)",
            "sender_id": 850,
            "sender_realm_str": "recurse",
            "stream_id": 31151,
            "subject": "What education should RC provide?",
            "submessages": [],
