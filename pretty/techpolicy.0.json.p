        {
            "avatar_url": "https://secure.gravatar.com/avatar/4f5279e356a105513701942b5b6af05f?d=identicon&version=1",
            "client": "website",
            "content": "<p>These days though, they have been forced to become a technology organization.</p>",
            "content_type": "text/html",
            "display_recipient": "tech policy",
            "flags": [
                "read",
                "historical"
            ],
            "id": 6642038,
            "is_me_message": false,
