        {
            "avatar_url": "https://secure.gravatar.com/avatar/4f5279e356a105513701942b5b6af05f?d=identicon&version=1",
            "client": "website",
            "content": "<p>@<strong>Jay Weisskopf</strong>, actually I would say that even technical people seem to believe that the NSA are basically wizards. I hear smart people say craaaaazy things about these agencies all the time.</p>",
            "content_type": "text/html",
            "display_recipient": "tech policy",
            "flags": [
                "read",
                "historical"
            ],
            "id": 6641405,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22718,
            "sender_email": "clemmer.alexander@gmail.com",
            "sender_full_name": "Alex Clemmer (S'13)",
            "sender_id": 2132,
            "sender_realm_str": "recurse",
            "stream_id": 20327,
            "subject": "prism",
            "submessages": [],
