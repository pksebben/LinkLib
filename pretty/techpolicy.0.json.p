        {
            "avatar_url": "https://secure.gravatar.com/avatar/fcd59db6d86365a6fba1ce0f9c7b8102?d=identicon&version=1",
            "client": "website",
            "content": "<p>The NSA has actively been building a graph analysis tool that could deal with this data on a large scale. </p>",
            "content_type": "text/html",
            "display_recipient": "tech policy",
            "flags": [
                "read",
                "historical"
            ],
            "id": 6634443,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22718,
            "sender_email": "zack@zacharymaril.com",
            "sender_full_name": "Zack Maril (S'13)",
            "sender_id": 2130,
            "sender_realm_str": "recurse",
            "stream_id": 20327,
            "subject": "prism",
