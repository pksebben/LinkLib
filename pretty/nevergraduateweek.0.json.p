            "sender_email": "jdherg@gmail.com",
            "sender_full_name": "John Hergenroeder (he) (SP2'15)",
            "sender_id": 7851,
            "sender_realm_str": "recurse",
            "stream_id": 100764,
            "subject": "Making the alumni experience better",
            "submessages": [],
            "timestamp": 1494052210,
