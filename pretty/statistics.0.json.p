        {
            "avatar_url": "https://secure.gravatar.com/avatar/fcd59db6d86365a6fba1ce0f9c7b8102?d=identicon&version=1",
            "client": "website",
            "content": "<p>Continuous is a little trickier, but eyeballing the histogram of A and fitting a distribution to it shouldn't be too hard. </p>",
            "content_type": "text/html",
            "display_recipient": "statistics",
            "flags": [
                "read",
                "historical"
            ],
