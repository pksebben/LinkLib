        {
            "avatar_url": "https://secure.gravatar.com/avatar/282da691709f4715fc3e0ff6bebd278b?d=identicon&version=1",
            "client": "website",
            "content": "<p>Why do you encounter solutions multiple times?</p>",
            "content_type": "text/html",
            "display_recipient": "friday-jobs-prep",
            "flags": [
                "read",
                "historical"
            ],
            "id": 17828909,
            "is_me_message": false,
