        {
            "avatar_url": "https://secure.gravatar.com/avatar/4d074af6cc341ef51b25bb2f738cdb11?d=identicon&version=1",
            "client": "website",
            "content": "<p>Why does upcasting fill in 1s? That seems wrong to me.</p>",
            "content_type": "text/html",
            "display_recipient": "small answers",
            "flags": [
                "read",
                "historical"
            ],
            "id": 131082301,
            "is_me_message": false,
