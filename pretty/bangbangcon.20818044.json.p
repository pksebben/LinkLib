        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/6527e077e84e20214d5373101ea15910d6b6f7a3?x=x&version=1",
            "client": "website",
            "content": "<p>That's all good to know. Thanks <span class=\"user-mention\" data-user-email=\"lindsey@composition.al\">@Lindsey Kuper</span>.</p>",
            "content_type": "text/html",
            "display_recipient": "bangbangcon",
            "flags": [
                "read",
                "historical"
            ],
            "id": 56113060,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 31759,
