        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>I like that approach, <span class=\"user-mention\" data-user-email=\"scott@scott.mn\">@Scott Feeney (W'13)</span>!</p>",
            "content_type": "text/html",
            "display_recipient": "bangbangcon",
