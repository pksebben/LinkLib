        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>there have already been a couple of loosely-!!Con-inspired events in other cities, which I find encouraging.</p>",
            "content_type": "text/html",
            "display_recipient": "bangbangcon",
            "flags": [
                "read",
                "historical"
            ],
            "id": 56114401,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 31759,
            "sender_email": "lindsey@composition.al",
            "sender_full_name": "Lindsey Kuper (she)",
            "sender_id": 2170,
            "sender_realm_str": "recurse",
            "stream_id": 25119,
            "subject": "registration lottery",
            "submessages": [],
