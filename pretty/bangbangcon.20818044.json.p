        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/e5845e045f9bcd4c95b82b01366893ed8b219f00?x=x&version=1",
            "client": "website",
            "content": "<p>What is the one thing?</p>",
            "content_type": "text/html",
            "display_recipient": "bangbangcon",
            "flags": [
                "read",
                "historical"
            ],
            "id": 56018924,
            "is_me_message": false,
            "reactions": [],
