        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/661412d42ecbf0c0bd354116859a5af3562f8e5f?x=x&version=4",
            "client": "website",
            "content": "<p>it\u2019s fine and dandy that we tell each other stories, but they shouldn\u2019t be our primary measure for people and actual situations, because they\u2019re always 1d, 1 pov, and then we use them for grading people.</p>",
            "content_type": "text/html",
            "display_recipient": "beanbags",
            "flags": [
                "read",
                "historical"
            ],
            "id": 165352870,
            "is_me_message": false,
            "last_edit_timestamp": 1557507914,
            "reactions": [],
