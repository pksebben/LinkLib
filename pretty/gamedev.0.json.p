            "sender_email": "ghthor@gmail.com",
            "sender_full_name": "Will Drengwitz (he) (W1'17)",
            "sender_id": 7099,
            "sender_realm_str": "recurse",
            "stream_id": 25341,
            "subject": "Just One Boss",
