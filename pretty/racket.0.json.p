        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-id=\"5866\">@Michael Robert Arntzenius (S'14)</span> might know about this.</p>",
            "content_type": "text/html",
            "display_recipient": "racket",
