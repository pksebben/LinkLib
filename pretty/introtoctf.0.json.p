        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/3c0275c73d9c05a4a2e092649b4deb2a5a79be07?x=x&version=2",
            "client": "website",
            "content": "<p>and of course all of that assumes that there's a <code>flag.txt</code> file that we can open and read</p>",
            "content_type": "text/html",
            "display_recipient": "intro to ctf",
            "flags": [
                "read",
                "historical"
            ],
            "id": 136518273,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 102161,
            "sender_email": "bittmanmax@gmail.com",
            "sender_full_name": "Max Bittman (he) (S2'18)",
            "sender_id": 120392,
            "sender_realm_str": "recurse",
            "stream_id": 100490,
