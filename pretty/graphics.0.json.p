        {
            "avatar_url": "https://secure.gravatar.com/avatar/4b16b09c5f3bf91a8ea65189e3b05f57?d=identicon&version=1",
            "client": "website",
            "content": "<p>maybe <span class=\"user-mention\" data-user-id=\"9335\">@Adrien Lamarque (they) (S1'16)</span> has some thoughts :)</p>",
            "content_type": "text/html",
            "display_recipient": "graphics",
            "flags": [
                "read",
                "historical"
            ],
            "id": 169257473,
            "is_me_message": false,
            "reactions": [],
