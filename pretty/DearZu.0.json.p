            "id": 202467673,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 421563,
            "sender_email": "mai@recurse.com",
            "sender_full_name": "Mai Schwartz",
            "sender_id": 225648,
            "sender_realm_str": "recurse",
            "stream_id": 224644,
            "subject": "programmer's block",
            "submessages": [],
