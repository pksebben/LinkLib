        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/0d3a1755b1e2bb0552c4fbb42bd02ef0?d=identicon&version=1",
            "client": "website",
            "content": "<p>Don't we need to change make-sum and make-product to construct a different representation which the selectors then use?  It just says that deriv doesn't change.</p>",
            "content_type": "text/html",
            "display_recipient": "sicp study group",
            "flags": [
                "read",
                "historical"
            ],
            "id": 13390105,
            "is_me_message": false,
