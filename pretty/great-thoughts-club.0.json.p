            "reactions": [],
            "recipient_id": 182180,
            "sender_email": "jean@jeancochrane.com",
            "sender_full_name": "Jean Cochrane (they) (F2'18)",
            "sender_id": 130298,
            "sender_realm_str": "recurse",
            "stream_id": 143756,
            "subject": "Reading groups",
            "submessages": [],
