        {
            "avatar_url": "https://secure.gravatar.com/avatar/4d074af6cc341ef51b25bb2f738cdb11?d=identicon&version=1",
            "client": "website",
            "content": "<p>see <a href=\"https://github.com/nh2/static-haskell-nix\" target=\"_blank\" title=\"https://github.com/nh2/static-haskell-nix\">https://github.com/nh2/static-haskell-nix</a> for an example</p>",
            "content_type": "text/html",
            "display_recipient": "heap community cluster",
            "flags": [
                "read",
                "historical"
            ],
            "id": 128282953,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 116688,
            "sender_email": "vaibhavsagar@gmail.com",
            "sender_full_name": "Vaibhav Sagar (he) (F1'16)",
            "sender_id": 100041,
            "sender_realm_str": "recurse",
            "stream_id": 108202,
            "subject": "rootless containers (runc / docker?)",
            "submessages": [],
            "timestamp": 1529376855,
