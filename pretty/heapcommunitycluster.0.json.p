        {
            "avatar_url": "https://secure.gravatar.com/avatar/4d074af6cc341ef51b25bb2f738cdb11?d=identicon&version=1",
            "client": "website",
            "content": "<p>but that seems to have the downside of depending on an old <code>glibc</code> that might not have everything we want</p>",
            "content_type": "text/html",
            "display_recipient": "heap community cluster",
            "flags": [
                "read",
                "historical"
            ],
            "id": 128283417,
            "is_me_message": false,
