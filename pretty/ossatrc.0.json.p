        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/541aab26abf25ac195df55d7e6c0f2daf3f00c70?x=x&version=1",
            "client": "website",
            "content": "<p>By which I mean: are you afraid for privacy reasons, afraid of saying the wrong thing, afraid of something else? We can help figure out how to address concerns</p>",
            "content_type": "text/html",
            "display_recipient": "oss at rc",
            "flags": [
                "read",
                "historical"
            ],
            "id": 27739999,
            "is_me_message": false,
