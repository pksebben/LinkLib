        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/0fc5778d097c3ce0d085a4e5d2115fdf89a5ce55?x=x&version=1",
            "client": "website",
            "content": "<p>Oh hey, I was wondering whether this would happen again.  I didn't hear much about folks' experiences with the first round but it looks like it must have been reasonably successful :)</p>",
            "content_type": "text/html",
            "display_recipient": "oss at rc",
            "flags": [
                "read",
                "historical"
            ],
            "id": 46549557,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 20937,
            "sender_email": "mindy.preston@yomimono.org",
            "sender_full_name": "Mindy Preston (she) (F2'19)",
            "sender_id": 4994,
            "sender_realm_str": "recurse",
            "stream_id": 19393,
            "subject": "stripe open source retreat applications open",
            "submessages": [],
            "timestamp": 1441374730,
            "topic_links": [],
