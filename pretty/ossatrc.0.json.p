            "avatar_url": "https://secure.gravatar.com/avatar/80c37b7308df099b8b9ec660146cf3da?d=identicon&version=1",
            "client": "website",
            "content": "<p><a href=\"https://stripe.com/blog/open-source-retreat-2016\" target=\"_blank\" title=\"https://stripe.com/blog/open-source-retreat-2016\">https://stripe.com/blog/open-source-retreat-2016</a></p>",
            "content_type": "text/html",
