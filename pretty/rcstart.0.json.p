            "sender_full_name": "David P Branner (SP2'17)",
            "sender_id": 850,
            "sender_realm_str": "recurse",
            "stream_id": 32331,
            "subject": "Path to retreat for starters",
            "submessages": [],
            "timestamp": 1463422047,
            "topic_links": [],
