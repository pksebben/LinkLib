        {
            "avatar_url": "https://secure.gravatar.com/avatar/dd9aceaf17982bc33972b3bb8701cd19?d=identicon&version=1",
            "client": "website",
            "content": "<p>No - for instance, if you rebase branches a lot, the repository you're cloning from will keep those old commits around until a <code>git gc</code> happens</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 38078464,
            "is_me_message": false,
