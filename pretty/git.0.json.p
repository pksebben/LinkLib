        {
            "avatar_url": "https://secure.gravatar.com/avatar/1a85d4a005d12db6858a848cb936f21d?d=identicon&version=1",
            "client": "website",
            "content": "<p>so it's not really a content addressable store, nor can it use git commands</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 102296002,
            "is_me_message": false,
            "reactions": [],
