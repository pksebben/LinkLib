        {
            "avatar_url": "https://secure.gravatar.com/avatar/9e0096ad9f376eb9f609372bc87542e4?d=identicon&version=1",
            "client": "website",
            "content": "<p>I don't think that's the right approach?</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 105860793,
            "is_me_message": false,
            "reactions": [],
