        {
            "avatar_url": "https://secure.gravatar.com/avatar/4d074af6cc341ef51b25bb2f738cdb11?d=identicon&version=1",
            "client": "website",
            "content": "<p>Is your project git-compatible?</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 102295957,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22657,
            "sender_email": "vaibhavsagar@gmail.com",
            "sender_full_name": "Vaibhav Sagar (he) (F1'16)",
            "sender_id": 100041,
            "sender_realm_str": "recurse",
            "stream_id": 20291,
            "subject": "Noms",
