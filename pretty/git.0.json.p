            "type": "stream"
        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/4401c7ea9c62aa8e62f7c8907b1db771?d=identicon&version=1",
            "client": "website",
            "content": "<p>Followed by a <code>git push -f origin skatztest</code>. Since I'm the only one using this branch (and there are only 2 other people on my team) hopefully force pushing won't be a problem.</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 50389274,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22657,
            "sender_email": "katz.stephenj@gmail.com",
            "sender_full_name": "Steve Katz (he) (F'13)",
