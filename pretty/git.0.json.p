        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/423a5327ed896db2dfd052de561709d0e6ae2689?x=x&version=1",
            "client": "website",
            "content": "<p>This is true, but if objects are identified by secure hash, as they are in the git object store, then this seems impossible to turn into a vulnerability, and would <em>super rarely</em> allow deduplication.</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 212789626,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22657,
            "sender_email": "casey@rodarmor.com",
            "sender_full_name": "Casey Rodarmor (SP2'18)",
            "sender_id": 6935,
            "sender_realm_str": "recurse",
