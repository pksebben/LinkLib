        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/1a85d4a005d12db6858a848cb936f21d?d=identicon&version=1",
            "client": "website",
            "content": "<p>One part of noms I'm really happy about is how they use type unions to represent different schema versions in code. So If you update your schema you'll have a type like  \"type Blah = Blah1 | Blah2\"</p>",
            "content_type": "text/html",
            "display_recipient": "git",
            "flags": [
                "read",
                "historical"
            ],
            "id": 102295959,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 22657,
