        {
            "avatar_url": "https://secure.gravatar.com/avatar/3f4785d95e43ecfb2f20d49e6ec824dc?d=identicon&version=1",
            "client": "website",
            "content": "<p>Ooh, thanks for the link, <span class=\"user-mention\" data-user-id=\"100791\">@Saul Pwanson (SP2'17)</span>!</p>",
            "content_type": "text/html",
