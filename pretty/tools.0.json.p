        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/2b3883a91e0e0a59f3998bcc07ca66d2e9e760a6?x=x&version=1",
            "client": "website",
            "content": "<p><a href=\"https://github.com/tpope/vim-vinegar\" target=\"_blank\" title=\"https://github.com/tpope/vim-vinegar\">https://github.com/tpope/vim-vinegar</a> for directory navigation, <a href=\"https://github.com/ctrlpvim/ctrlp.vim\" target=\"_blank\" title=\"https://github.com/ctrlpvim/ctrlp.vim\">https://github.com/ctrlpvim/ctrlp.vim</a> for project navigation</p>",
