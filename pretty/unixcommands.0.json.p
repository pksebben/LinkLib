        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/e30b9e7d531d60960da4c58b24cf7e7c5aff03f0?x=x&version=5",
            "client": "ZulipElectron",
            "content": "<p><span class=\"user-mention\" data-user-email=\"alokbeniwal@gmail.com\" data-user-id=\"101076\">@Alok Singh (S1'17)</span> could you detail how the fzf cd widget works in terms of wiring it up and triggering it?</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
                "historical"
            ],
            "id": 119859446,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 23402,
            "sender_email": "pete@peterlyons.com",
            "sender_full_name": "Peter Lyons (he) (W1'17)",
            "sender_id": 103052,
            "sender_realm_str": "recurse",
            "stream_id": 20565,
            "subject": "shell snippet show and tell",
            "submessages": [],
            "timestamp": 1513446336,
            "topic_links": [],
