        {
            "avatar_url": "https://secure.gravatar.com/avatar/dd9aceaf17982bc33972b3bb8701cd19?d=identicon&version=1",
            "client": "website",
            "content": "<p>Doing this is a bit complicated, but the general idea is that <code>PROMPT_COMMAND</code> is an environment variable that, if set, gets evaluated just before drawing the prompt</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
                "historical"
            ],
            "id": 37408864,
            "is_me_message": false,
            "reactions": [],
