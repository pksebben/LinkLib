        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/cdcdcd18a8a06824768b7b3d6c04eed0a3f0529c?x=x&version=2",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"hanglu.su@gmail.com\" data-user-id=\"101107\">@Hang Lu Su (S1'17)</span> Do you have an example of an external application not needing permission?</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
