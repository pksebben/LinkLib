            "topic_links": [],
            "type": "stream"
        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/7857f26c1ef2e9bdbfa843f9087710f7?d=identicon&version=1",
            "client": "ZulipElectron",
            "content": "<p>FWIW, this is the relevant page in the bash docs: <a href=\"https://www.gnu.org/software/bash/manual/html_node/Arithmetic-Expansion.html\" target=\"_blank\" title=\"https://www.gnu.org/software/bash/manual/html_node/Arithmetic-Expansion.html\">https://www.gnu.org/software/bash/manual/html_node/Arithmetic-Expansion.html</a></p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
                "historical"
            ],
            "id": 186351324,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 23402,
            "sender_email": "nathan12343@gmail.com",
            "sender_full_name": "Nathan Goldbaum (he) (S1'19)",
            "sender_id": 221814,
            "sender_realm_str": "recurse",
            "stream_id": 20565,
            "subject": "Non-portable bash",
