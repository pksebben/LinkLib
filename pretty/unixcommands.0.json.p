        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/8d9ed5f596714f4f3946f0896c9247b7686db1f6?x=x&version=4",
            "client": "website",
            "content": "<p>My controversial opinion is that manually doing permissions management will eventually become less important as fewer people are sharing machines without containerization these days</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
