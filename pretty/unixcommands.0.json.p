            "type": "stream"
        },
        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/cbcb9589503e23026f6e55769709dc6a43465431?x=x&version=2",
            "client": "website",
            "content": "<p>It's not just bash, it's any script.</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
                "historical"
            ],
            "id": 111621678,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 23402,
            "sender_email": "alokbeniwal@gmail.com",
            "sender_full_name": "Alok Singh (S1'17)",
            "sender_id": 101076,
            "sender_realm_str": "recurse",
            "stream_id": 20565,
            "subject": "Why is chmod even a thing?",
