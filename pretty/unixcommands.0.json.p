            "type": "stream"
        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/5741d3f5ad703c633e2c62e564b1ef0e?d=identicon&version=1",
            "client": "website",
            "content": "<p>I forgot that you can pass paths to find as the first argument <span class=\"emoji emoji-1f926\" title=\"face palm\">:face_palm:</span>, thanks. I think it would be super if <code>find</code> could read files from stdin.</p>",
            "content_type": "text/html",
            "display_recipient": "unix commands",
            "flags": [
                "read",
                "historical"
            ],
            "id": 135055421,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 23402,
            "sender_email": "connor@cdosborn.com",
            "sender_full_name": "Connor Osborn (F1'14)",
            "sender_id": 6427,
            "sender_realm_str": "recurse",
            "stream_id": 20565,
            "subject": "`find` as a filter?",
