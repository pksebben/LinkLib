        {
            "avatar_url": "https://secure.gravatar.com/avatar/8884e5c98af40f03df713b25a2f6ddcf?d=identicon&version=1",
            "client": "website",
            "content": "<p>Note: you can use any editor as long as it has an OCaml mode, but merlin support is only there for emacs and vim.</p>",
            "content_type": "text/html",
            "display_recipient": "ocaml",
            "flags": [
                "read",
                "historical"
            ],
            "id": 11759053,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 25750,
            "sender_email": "yminsky@janestreet.com",
