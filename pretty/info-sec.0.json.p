        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/d495685876a5a195b6ef5fb9a269b6d0dcd5c5fe?x=x&version=1",
            "client": "ZulipiOS",
            "content": "<p>Typically, the OTP generating device is separate from the computer that might be compromised, eg. a dongle in your pocket. </p>",
            "content_type": "text/html",
            "display_recipient": "info-sec",
            "flags": [
                "read",
                "historical"
            ],
            "id": 17208335,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 31835,
            "sender_email": "pnf@podsnap.com",
            "sender_full_name": "Peter Fraenkel (he) (S'13)",
            "sender_id": 2142,
            "sender_realm_str": "recurse",
            "stream_id": 25135,
            "subject": "OTP",
            "submessages": [],
            "timestamp": 1392830904,
            "topic_links": [],
