        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/7fa3c19ae8d00435e94afd1ff19d92553c66eefa?x=x&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"pnf@podsnap.com\">@Peter Fraenkel (S'13)</span>, Is OTP any better than ssh-agent? If so, how?</p>",
            "content_type": "text/html",
            "display_recipient": "info-sec",
            "flags": [
                "read",
                "historical"
            ],
            "id": 17207788,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 31835,
