        {
            "avatar_url": "https://secure.gravatar.com/avatar/a6188a392c3f67335e6fa8f490844901?d=identicon&version=1",
            "client": "website",
            "content": "<p>Nov 2, not far from RC: <a href=\"http://www.meetup.com/OWASP-Brooklyn/events/228267143/\" target=\"_blank\" title=\"http://www.meetup.com/OWASP-Brooklyn/events/228267143/\">http://www.meetup.com/OWASP-Brooklyn/events/228267143/</a></p>",
            "content_type": "text/html",
            "display_recipient": "info-sec",
            "flags": [
                "read",
                "historical"
            ],
            "id": 102579236,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 31835,
            "sender_email": "meitarm@gmail.com",
            "sender_full_name": "May may (F2'16)",
            "sender_id": 100208,
            "sender_realm_str": "recurse",
            "stream_id": 25135,
            "subject": "Brooklyn OWASP meeting",
            "submessages": [],
            "timestamp": 1477259778,
            "topic_links": [],
