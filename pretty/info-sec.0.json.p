        {
            "avatar_url": "https://zulip-avatars.s3.amazonaws.com/13/885b8afeb10339344e0d2a1c588f78e26734013c?x=x&version=1",
            "client": "website",
            "content": "<p>Oh definitely! CT is an incredibly powerful tool and provides all sorts of additional checks on the CA system</p>",
            "content_type": "text/html",
            "display_recipient": "info-sec",
            "flags": [
                "read",
