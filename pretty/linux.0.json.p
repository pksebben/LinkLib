        {
            "avatar_url": "https://secure.gravatar.com/avatar/3ca604eb0c0cfa22b4a10fc739c056c8?d=identicon&version=1",
            "client": "website",
            "content": "<p><span class=\"user-mention\" data-user-email=\"jeffowler@gmail.com\">@Jeff Fowler (W'14)</span> arch sounds reasonable for getting hands-on these days, though the major thing with any distro is to use it.</p>",
            "content_type": "text/html",
            "display_recipient": "linux",
            "flags": [
                "read",
