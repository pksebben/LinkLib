            ],
            "id": 109582863,
            "is_me_message": false,
            "last_edit_timestamp": 1492133795,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "lindsey@composition.al",
            "sender_full_name": "Lindsey Kuper (she)",
            "sender_id": 2170,
            "sender_realm_str": "recurse",
            "stream_id": 26031,
            "subject": "fork of replication thread",
            "submessages": [],
