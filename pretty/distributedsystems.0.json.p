        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>Breaking news: as I continue to read about OpenStack, I am delighted to learn that there's something called a \"machine flavor\".</p>",
            "content_type": "text/html",
            "display_recipient": "distributed systems",
            "flags": [
                "read",
                "historical"
            ],
            "id": 56590879,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "lindsey@composition.al",
            "sender_full_name": "Lindsey Kuper (she)",
