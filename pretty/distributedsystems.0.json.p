        {
            "avatar_url": "https://secure.gravatar.com/avatar/3d7b72d70ff07f8186126a4464bc6166?d=identicon&version=1",
            "client": "website",
            "content": "<p>I think pre fetching is also super interesting. AFAIK we don't do any right now.</p>",
            "content_type": "text/html",
            "display_recipient": "distributed systems",
            "flags": [
                "read",
                "historical"
            ],
            "id": 205078119,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "emmbeezee@gmail.com",
            "sender_full_name": "Maggie Zhou (she) (S'13)",
            "sender_id": 2161,
            "sender_realm_str": "recurse",
            "stream_id": 26031,
