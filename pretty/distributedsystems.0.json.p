        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>This is really irritating me. :(</p>",
            "content_type": "text/html",
            "display_recipient": "distributed systems",
            "flags": [
                "read",
                "historical"
            ],
            "id": 57128738,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "lindsey@composition.al",
            "sender_full_name": "Lindsey Kuper (she)",
            "sender_id": 2170,
            "sender_realm_str": "recurse",
            "stream_id": 26031,
            "subject": "primary-backup vs. state machine replication",
