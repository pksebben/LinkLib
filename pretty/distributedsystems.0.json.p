            "id": 102514208,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "juliano.solanho@gmail.com",
            "sender_full_name": "Juliano Bortolozzo Solanho (he) (F1'16)",
            "sender_id": 100035,
            "sender_realm_str": "recurse",
            "stream_id": 26031,
