        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/4f5279e356a105513701942b5b6af05f?d=identicon&version=1",
            "client": "website",
            "content": "<p>In particular, have a look at section 2.2 of the second link, which explicitly explains step-by-step how replication works in both systems, and even includes fun little graphs.</p>",
            "content_type": "text/html",
            "display_recipient": "distributed systems",
            "flags": [
                "read",
                "historical"
            ],
            "id": 57128448,
            "is_me_message": false,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "clemmer.alexander@gmail.com",
            "sender_full_name": "Alex Clemmer (S'13)",
            "sender_id": 2132,
            "sender_realm_str": "recurse",
            "stream_id": 26031,
            "subject": "primary-backup vs. state machine replication",
            "submessages": [],
            "timestamp": 1463009860,
