            "id": 116818661,
            "is_me_message": false,
            "last_edit_timestamp": 1507425124,
            "reactions": [],
            "recipient_id": 35669,
            "sender_email": "rudichen@gmail.com",
            "sender_full_name": "Rudi Chen (S1'17)",
