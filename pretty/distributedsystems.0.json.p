            "type": "stream"
        },
        {
            "avatar_url": "https://secure.gravatar.com/avatar/57ad6b289d2b066f6497c2c7a542f246?d=identicon&version=1",
            "client": "website",
            "content": "<p>Yeah, the \"Vive la difference\" paper has been on my pile for a while.</p>",
            "content_type": "text/html",
            "display_recipient": "distributed systems",
            "flags": [
                "read",
                "historical"
            ],
